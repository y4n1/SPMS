<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1" 
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">

    
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>user</from-outcome>
            <to-view-id>index.xhtml</to-view-id>
            <redirect></redirect>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/student/studentPage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>projectPropose</from-outcome>
            <to-view-id>/student/projectPropose.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ProjectbySpv</from-outcome>
            <to-view-id>/student/ProjectbySpv.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>ProjectbyTitle</from-outcome>
            <to-view-id>/student/ProjectbyTitle.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>RequestCancel</from-outcome>
            <to-view-id>/student/RequestCancel.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>   
    <navigation-rule>
        <from-view-id>/login.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Supervisor</from-outcome>
            <to-view-id>/supervisor/supervisorPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/supervisor/supervisorPage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>projectTopic</from-outcome>
            <to-view-id>/supervisor/projectTopicSpv.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>projectRegistration</from-outcome>
            <to-view-id>/supervisor/projectRegistrationSpv.xhtml</to-view-id>
        </navigation-case>
         <navigation-case>
            <from-outcome>projectApproval</from-outcome>
            <to-view-id>/supervisor/projectApproval.xhtml</to-view-id>
        </navigation-case>
        
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/projectTopic.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>index</from-outcome>
            <to-view-id>index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>supervisorPage</from-outcome>
            <to-view-id>/supervisor/supervisorPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/supervisor/projectRegistrationSpv.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>projectTopicspv</from-outcome>
            <to-view-id>/supervisor/projectTopicSpv.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>supervisorPage</from-outcome>
            <to-view-id>/supervisor/supervisorPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
